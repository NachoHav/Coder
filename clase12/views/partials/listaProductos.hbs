
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col" class="text-center">Foto</th>
    </tr>
  </thead>
 
<tbody>
   {{#each productos}}
    <tr>
      <th scope="row"><p>{{title}}</p></th>
      <td>${{price}}</td>
      <td class="text-center"><img src={{thumbnail}}  width="50" height="50"></td>
    </tr>
    {{/each}}
  </tbody>

</table>

